{
  "name": "nestjs-doctor",
  "version": "0.1.1",
  "description": "Diagnostic CLI tool that scans NestJS codebases and produces a health score with actionable diagnostics",
  "type": "module",
  "bin": {
    "nestjs-doctor": "./dist/cli/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.cjs"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nestjs",
    "diagnostic",
    "lint",
    "health-check",
    "architecture",
    "security",
    "best-practices"
  ],
  "author": "RoloBits",
  "license": "MIT",
  "devDependencies": {
    "tsdown": "^0.20.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "clipboardy": "^4.0.0",
    "ora": "9.3.0",
    "picocolors": "^1.1.0",
    "tinyglobby": "^0.2.0",
    "ts-morph": "^25.0.0"
  }
}
