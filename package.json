{
  "name": "nestjs-doctor-monorepo",
  "private": true,
  "packageManager": "pnpm@10.2.0",
  "scripts": {
    "dev": "pnpm --filter nestjs-doctor run dev",
    "build": "pnpm --filter nestjs-doctor run build",
    "test": "pnpm --filter nestjs-doctor run test",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "typecheck": "pnpm --filter nestjs-doctor run typecheck",
    "build:website": "pnpm --filter website run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "@changesets/cli": "^2.27.0",
    "husky": "9.1.7",
    "typescript": "^5.7.0",
    "ultracite": "7.2.3"
  }
}
